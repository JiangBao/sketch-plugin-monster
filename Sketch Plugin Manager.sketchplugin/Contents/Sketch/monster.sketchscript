@import 'lib/utils.js';
@import 'lib/PluginHandler.js';
@import 'lib/MochaJSDelegate.js';
@import 'lib/Panel.js';

function monsterInit(context) {
  var pluginHandler = new PluginHandler();
  var panel = new Panel({
    context: context,
    template: 'index'
  }, { a : 1 }, function () {
    panel.execute('document.write(\'Execute callback successfully!\')');
  });

  panel.show();
}
